$yellow: #FFC72C;\n$darkgray: #27251F;\n$red: #DA291C;\n$gray: #E5E5E5;\n\n$phone: 480px;\n$tablet: 640px;\n$desktop: 1024px;\n\n\nbody {\n  margin: 0;\n  padding: 0;\n  font-family: 'Roboto Condensed', sans-serif;\n  color: $darkgray;\n  background: $yellow;\n}\n\n/* TODO: NAV is not placed to true center */\n\nheader {\n  background: $darkgray;\n\n  .navbar {\n    margin: 0 auto;\n    flex-direction: column;\n    display: flex;\n    align-items: center;\n\n    nav {\n      display: flex;\n      flex-wrap: wrap;\n      justify-content: center;\n\n      a {\n        font-size: 1.25rem;\n        color: #fff;\n        text-decoration: none;\n        padding: 0.5rem;\n        margin: 0.5rem 0;\n        box-sizing: border-box;\n        border-bottom: 2px solid transparent;\n\n        &:hover {\n          background: $red;\n          border-bottom: 2px solid $yellow;\n        }\n\n        &:focus,\n        :active {\n          background: $yellow;\n          border-bottom: 2px solid $red;\n          color: $darkgray;\n        }\n      }\n    }\n\n    .search {\n      display: flex;\n      align-items: center;\n      background: #fff;\n\n      input {\n        padding: 0.5rem;\n        border: 0;\n        outline: none;\n        margin-left: 5px;\n      }\n\n      .search-icon {\n        padding: 0 0.5rem;\n      }\n    }\n  }\n}\n\n/*** MAIN ****/\n\nmain {\n  max-width: 1200px;\n  margin: 0 auto;\n  background: #fff;\n\n  .hero-image {\n    height: 300px;\n    background-image: linear-gradient(to bottom, $darkgray, #00000000), url(\"../img/bread.webp\");\n    background-size: cover;\n    background-position: center;\n    text-align: center;\n    padding: 0 1rem 1rem 1rem;\n    display: flex;\n    flex-direction: column;\n    justify-content: space-between;\n    color: #fff;\n\n\n\n    h1 {\n      display: inline-block;\n      font-size: 3rem;\n      margin: 1rem;\n    }\n\n    .info {\n      display: flex;\n      justify-content: space-evenly;\n\n      .info-data-title {\n        font-weight: bold;\n        background-color: #27272750;\n        padding: 1rem;\n        border-radius: 5px;\n\n        .info-data-text {\n          font-weight: normal;\n        }\n      }\n    }\n  }\n\n\n  .content {\n    display: flex;\n    flex-direction: column;\n    font-size: 1.2rem;\n\n    aside,\n    article {\n      h2 {\n        padding: 1rem;\n        border-left: 0.5rem solid $red;\n        margin: 1rem;\n      }\n    }\n\n\n    aside {\n      ul {\n        li {\n          list-style: circle;\n        }\n      }\n    }\n\n    article {\n      p {\n        padding: 0.5rem 1rem;\n      }\n    }\n  }\n\n}\n\n\n#up-btn {\n  display: none;\n  position: fixed;\n  bottom: 20px;\n  right: 20px;\n  font-size: 18px;\n  background-color: #FFC72C50;\n  padding: 0.5rem;\n  border-radius: 5px;\n  transition: all 200ms;\n\n  a {\n    text-decoration: none;\n  }\n\n  &:hover {\n    background-color: $red;\n  }\n}\n\n\n\n/* On screens that are 768px or less */\n@media screen and (min-width: $tablet) {\n  main {\n    .hero-image {\n      height: 500px;\n      align-items: center;\n\n\n      .info>.info-data-title {\n        margin: 0 1rem;\n      }\n    }\n  }\n}\n\n@media screen and (min-width: $desktop) {\n  header {\n\n    .navbar {\n      max-width: 1200px;\n      flex-direction: row;\n      justify-content: space-between;\n      padding: 0;\n\n      nav {\n        a {\n          padding: 1.5rem 1rem;\n          margin: 0;\n        }\n      }\n    }\n  }\n\n\n  main {\n    .content {\n      flex-direction: row;\n      gap: 1rem;\n      margin-top: 1rem;\n\n      aside {\n\n        ul {\n          width: max-content;\n\n        }\n\n        h2 {\n          margin: 0;\n        }\n\n      }\n\n      article {\n\n        h2 {\n          margin: 0;\n        }\n      }\n    }\n\n  }\n\n}"},"lineCount":null}},"error":null,"hash":"8d5eb5dab62bb292483e6e0998bef9d6","cacheData":{"env":{}}}